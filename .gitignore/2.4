#include <iostream>

using namespace std;


int massiv( int*arr, int first, int last)
{
    int temp1=arr[0];
    int temp2=arr[1];
    int temp3;
    for(;first<last;++first)
{
    temp3=temp1+temp2;
    temp1=temp3;
    temp2=temp3;
    cout<<temp3<<endl;
}
}
int main()
{
	int array[8]{1,2};
	cout<<massiv(array,2,8);
	return 0;

}
