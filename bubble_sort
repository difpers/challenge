#include <iostream>
using namespace std;

void bubble_sort(int*arr,int size)
{
	for(int i=0; i<size;++i)
	{
		for (int j = 0; j<size-1-i; ++j)
	{
	if(arr[j]<arr[j+1])
	{
	swap(arr[j],arr[j+1]);
	
}}
}
}
void f( int*arr, int size)
{
	for (int k=0; k<size; ++k)
	{cout<<arr[k]<< " ";}
}
 
int main() {
	int array [13]{5,2,3,4,1,6,13,8,9,10,11,12,7};
	bubble_sort(array,13);
	f( array,13);
	return 0;
}
